{
  "parserOptions" : {
    "ecmaVerion": 6,
    "sourceType": "module",
    "ecmaFeatures" : {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "camelcase": [2,{ "properties": "always"}],
    "dot-location": [2,"property"],
    "no-eval": 2,
    "no-div-regex": 2,
    "no-eq-null": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true}],
    "no-nested-ternary": 2,
    "no-var": 2
  }
}
